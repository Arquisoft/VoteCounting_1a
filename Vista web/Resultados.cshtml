@{
    Layout = null;

    IList<KeyValuePair<string, int?>> votos = new List<KeyValuePair<string, int?>>();

    string url = "Server=tcp:aswneo.database.windows.net,1433;Database=ASWneo;User ID=carlubian@aswneo;Password=H3lloKitty;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;";
    System.Data.SqlClient.SqlConnection con = new System.Data.SqlClient.SqlConnection(url);

    string consulta = "SElECT * FROM Votos";
    System.Data.SqlClient.SqlCommand cmd = new System.Data.SqlClient.SqlCommand(consulta, con);

    con.Open();

    System.Data.SqlClient.SqlDataReader rs = cmd.ExecuteReader();
    while(rs.Read())
    {
        votos.Add(new KeyValuePair<string, int?>(rs[0] as string, rs[1] as int?));
    }
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <table>
            <thead>
                <tr>
                    <th>Opción</th>
                    <th>Nº de votos</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var kvp in votos)
                {
                    <tr>
                        <td>@kvp.Key</td>
                        <td>@kvp.Value</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <footer>
        <a href="~/Default.aspx">Volver</a>
    </footer>
</body>
</html>
